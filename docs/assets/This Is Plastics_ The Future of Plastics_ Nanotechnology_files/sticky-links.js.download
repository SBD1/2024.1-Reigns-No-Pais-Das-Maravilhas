$("#sticky-links ul li .text a").each(function(){
  	var name = $(this).text();
	var url = $(this).attr("href");
  	var target = $(this).attr("target");
  
	$(this).parents("li").attr("data-name", name);
  	$(this).parents("li").attr("data-url", url);
  	$(this).parents("li").attr("data-target", target);
	
	  
	$(this).parents("li > .icon").attr("data-name", name);
  	$(this).parents("li > .icon").attr("data-url", url);
  	$(this).parents("li > .icon").attr("data-target", target);
});


$("#sticky-links ul li").click(function(e){
	ga('send', 'event', 'Sticky Links', $(this).data('name'));
	if($(this).data('name') === "Subscribe"){
			$(this).find("a").attr("href", "");
			$('html, body').animate({
				scrollTop: $('#subscribe').offset().top - 80 
			}, 'slow');
	}else{
		URL = $(this).data('url');
		target = ($(this).data('target')) ? $(this).data('target') : "_self" ;
		window.open(URL, target);	
	}
	
	e.preventDefault();
	
});